APP_ENV=development
APP_DEBUG=true
APP_URL=https://example.com
LOG_CHANNEL=stack

TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_WEBHOOK_SECRET=replace_with_secret

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=quiz_bot
DB_USERNAME=root
DB_PASSWORD=secret

CACHE_DRIVER=array
QUEUE_CONNECTION=sync

# Список Telegram ID администраторов (через запятую)
# Чтобы узнать свой Telegram ID, напишите боту @userinfobot
ADMIN_TELEGRAM_IDS=

# URL Mini App (точный origin)
WEBAPP_URL=https://your-domain.com/webapp/

# WebSocket (для real-time)
WEBSOCKET_HOST=127.0.0.1
WEBSOCKET_PORT=8090
WEBSOCKET_TICKET_SECRET=<длинный_случайный_секрет>
INIT_DATA_TTL_SECONDS=300
WEBSOCKET_TICKET_TTL_SECONDS=300
