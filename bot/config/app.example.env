APP_ENV=development
APP_DEBUG=true
APP_URL=https://example.com
LOG_CHANNEL=stack

TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_WEBHOOK_SECRET=replace_with_secret

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=quiz_bot
