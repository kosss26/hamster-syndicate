ADMIN_TELEGRAM_IDS=

# URL картинки для вопросов в дуэлях (опционально)
# Если задан, картинка будет отправляться вместе с каждым вопросом
# Пример: DUEL_QUESTION_IMAGE_URL=https://example.com/image.jpg
DUEL_QUESTION_IMAGE_URL=

