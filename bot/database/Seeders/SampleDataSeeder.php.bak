            foreach ($chapterData['steps'] as $index => $stepData) {
                // –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ç–∏–ø —à–∞–≥–∞
                $stepType = StoryStep::TYPE_NARRATIVE;
                if (isset($stepData['question']) && $stepData['question'] !== null) {
                    $stepType = StoryStep::TYPE_QUESTION;
                } elseif (isset($stepData['choice_options']) && !empty($stepData['choice_options'])) {
                    $stepType = StoryStep::TYPE_CHOICE;
                }

                $step = StoryStep::query()->updateOrCreate(
                    ['code' => $stepData['code']],
                    [
                        'chapter_id' => $chapter->getKey(),
                        'step_type' => $stepType,
                        'narrative_text' => $stepData['narrative'] ?? null,
                        'position' => $index + 1,
                        'branch_key' => null,
                        'reward_points' => $stepData['reward'] ?? 0,
                        'penalty_points' => $stepData['penalty'] ?? 0,
                        'transitions' => $stepData['transitions'] ?? null,
                        'choice_options' => $stepData['choice_options'] ?? null,
                    ]
                );

                // –ï—Å–ª–∏ —ç—Ç–æ —à–∞–≥ —Å –≤–æ–ø—Ä–æ—Å–æ–º –∏—Å—Ç–æ—Ä–∏–∏, —Å–æ–∑–¥–∞—ë–º StoryQuestion
                if ($stepType === StoryStep::TYPE_QUESTION && isset($stepData['story_question'])) {
                    $storyQuestionData = $stepData['story_question'];
                    
                    $storyQuestion = StoryQuestion::query()->updateOrCreate(
                        [
                            'story_step_id' => $step->getKey(),
                            'position' => 0,
                        ],
                        [
                            'question_text' => $storyQuestionData['question_text'],
                            'context_text' => $storyQuestionData['context_text'] ?? null,
                            'explanation' => $storyQuestionData['explanation'] ?? null,
                        ]
                    );

                    // –°–æ–∑–¥–∞—ë–º –æ—Ç–≤–µ—Ç—ã –¥–ª—è –≤–æ–ø—Ä–æ—Å–∞ –∏—Å—Ç–æ—Ä–∏–∏
                    if (isset($storyQuestionData['answers'])) {
                        StoryQuestionAnswer::query()
                            ->where('story_question_id', $storyQuestion->getKey())
                            ->delete();

                        foreach ($storyQuestionData['answers'] as $answerIndex => $answerData) {
                            StoryQuestionAnswer::query()->create([
                                'story_question_id' => $storyQuestion->getKey(),
                                'answer_text' => $answerData['text'],
                                'is_correct' => $answerData['correct'] ?? false,
                                'position' => $answerIndex,
                            ]);
                        }
                    }
                }
